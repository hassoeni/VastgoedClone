generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model VastgoedObj {
  id           Int      @id @default(autoincrement())
  createdAt    DateTime @default(now())
  naam         String   @unique
  energielabel String
  marktwaarde  Int
  gbo          Int
  postcode     String
  adress       String
  stad         String
  postedBy     User?    @relation(fields: [postedById], references: [id])
  postedById   Int?
}

model User {
  id       Int           @id @default(autoincrement())
  name     String
  email    String        @unique
  password String
  panden   VastgoedObj[] // links 
}
